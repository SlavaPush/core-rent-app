
{{!-- <form action="*" name="allPosts" method="POST"> --}}
  {{!-- <form action="" method="POST"> --}}

{{#each house}}
  <div class="postMain">
    <div ><h4>{{this.title}}</h4></div>
    <div >{{this.content}}</div>
    <div>{{this.number}}</div>
    <div >{{this.price}}</div>
    <div><button class="btn btn-primary" type="submit">Откликнуться</button></div>
  </div>
  <div>{{comment}}</div>
  <div>{{number}}</div>
  <div>{{price}}</div>
  <div><button class="btn btn-primary modal-trigger" data-target="modal1" type="button">Откликнуться</button></div>
</div>
<div id="modal{{id}}" class="modal">
  <div class="modal-content">
    <form action="alllist/confirm" name="posts" method="POST">
      <div class="windowAdd">
        <div class="input-field col s12">
          <input id="icon_telephone" type="tel" class="validate">
          <label for="icon_telephone">{{id}}</label>
        </div>
        <input type="hidden" name="inputID" value="{{id}}">
        <div><input type="datetime-local" placeholder="Выберите удобную Вам дату"></div>
        <div class="input-field col s12">

{{/each}}


